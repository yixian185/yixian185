import requests
url="https://www.sogou.com/web?query=mac"
headers={"User-Agent":"Mozilla/5.0 (Macintosh; Intel Mac OS X 10_15_5) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/106.0.0.0 Safari/537.36 Edg/106.0.1370.47"}   #以字典的形式设置请求头，处理反爬
resp=requests.get(url,headers=headers)
print(resp)  
print(resp.text) 
resp.close() 
